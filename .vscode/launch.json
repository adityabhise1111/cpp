{
    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Yeoman generator",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/yo/lib/cli.js",
            "args": [
                "generator"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Launch C++ Program",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/28ref/ref1", // Update this path to your executable
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/28ref", // Set the working directory to the folder containing ref1.cpp
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}